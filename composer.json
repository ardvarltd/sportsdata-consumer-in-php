{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/orm": "^2.10",
    "enqueue/async-event-dispatcher": "^0.10.8",
    "enqueue/enqueue-bundle": "^0.10.12",
    "enqueue/rdkafka": "^0.10.20",
    "sroze/messenger-enqueue-transport": "^0.5.1",
    "flix-tech/avro-serde-php": "^2.1.0",
    "ext-json": "*",
    "ext-rdkafka": "*",
    "flix-tech/avro-php": "^5.0.2"
  },
  "require-dev": {
    "doctrine/annotations": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
